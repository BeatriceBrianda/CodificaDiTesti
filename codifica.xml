	<text xml:lang="it" xml:id="Pag90-91-92" subtype="manuscript">
		<body>
			<div type="pagina" n="90">
				<pb n="90" facs="#P90Intera" />
				<ab type="pericope" n="1" xml:id="Pag90Pericope1">
					<lb xml:id="P90R1" n="1" facs="#P90Riga1" />
					<del rend="overstrike">ogni</del> balzo quale non ho
					<lb xml:id="P90R2" n="2" facs="#P90Riga2" />
					mai fatto in vita mia e				
					<lb xml:id="P90R3" n="3" facs="#P90Riga3" />
					si cominciò la volata per
					<lb xml:id="P90R4" n="4" facs="#P90Riga4" />
					la campagna.
				</ab>

				<ab type="pericope" n="2" xml:id="Pag90Pericope2">
					Giunti quasi
					<lb xml:id="P90R5" n="5" facs="#P90Riga5" />
					alla meta, preparai le bombe
					<lb xml:id="P90R6" n="6" facs="#P90Riga6" />
					a mano per il combattimento
					<lb xml:id="P90R7" n="7" facs="#P90Riga7" />
					e dichiarai ai compagni che
					<lb xml:id="P90R8" n="8" facs="#P90Riga8" />
					si mostravano molto 
					<lb xml:id="P90R9" n="9" facs="#P90Riga9" />
					compia<lb break="no"/>ciuti,
				</ab>
				<ab type="pericope" n="3" xml:id="Pag90Pericope3">
					ora mi accorgo di non
					<lb xml:id="P90R10" n="10" facs="#P90Riga10" />
					essere nato per fare il professore,
					<lb xml:id="P90R11" n="11" facs="#P90Riga11" />
					ma per fare il <choice><sic>gamster</sic> <corr resp="BB">gangster</corr></choice>
					<lb xml:id="P90R12" n="12" facs="#P90Riga12" />
					la sparator<damage agent="ink"><supplied>ia</supplied></damage> dal  <gap atLeast="5" atMost="7" unit="chars" reason="illegible"/>
					<lb xml:id="P90R13" n="13" facs="#P90Riga13" />
					e poi, quando balzai giù,
					<lb xml:id="P90R14" n="14" facs="#P90Riga14" />
					non vidi che gli ultimi
					<lb xml:id="P90R15" n="15" facs="#P90Riga15" />
					lembi dei <del rend="overstrike">fascisti</del> vestiti
					<lb xml:id="P90R16" n="16" facs="#P90Riga16" />
					dei fascisti che fuggivano
					<lb xml:id="P90R17" n="17" facs="#P90Riga17" />
					oltre il porticato del mercato
					<lb xml:id="P90R18" n="18" facs="#P90Riga18" />
					coperto. 	
				</ab>
				<ab type="pericope" n="4" xml:id="Pag90Pericope4">
					Spa<damage agent="ink"><supplied>r</supplied></damage>ar<damage agent="ink"><supplied>e</supplied></damage>
					<add place="above">nella schiena dei</add>
					<del rend="overstrike"><damage agent="ink"><supplied>sui</supplied></damage></del> fuggenti
					<lb xml:id="P90R19" n="19" facs="#P90Riga19" />
					l'ho sentito, è una cosa che
					<lb xml:id="P90R20" n="20" facs="#P90Riga20" />
					non saprò mai fare.
				</ab>
				<ab type="pericope" n="5" xml:id="Pag90Pericope5">
					La 
					<lb xml:id="P90R21" n="21" facs="#P90Riga21" />
					batta<lb break="no"/>glia mi parve terminata, le
					<lb xml:id="P90R22" n="22" facs="#P90Riga22" />
					donne uscivano dalle porte
				</ab>
			</div>

			<div type="pagina" n="91">
				<pb n="91" facs="#P91Intera" />
				
				<ab type="pericope" n="1" xml:id="Pag91Pericope1">

					<lb xml:id="P91R1" n="1" facs="#P91Riga1" />
					e il maresciallo dei carabinieri
					<lb xml:id="P91R2" n="2" facs="#P91Riga2" />
					passeggiava con un'aria piuttosto
					<lb xml:id="P91R3" n="3" facs="#P91Riga3" />
					ridicola, così ebbi il battesimo
					<lb xml:id="P91R4" n="4" facs="#P91Riga4" />
					del fuoco, per usare una frase
					<lb xml:id="P91R5" n="5" facs="#P91Riga5" />
					fatta <del rend="overstrike">piuttosto</del> infelice quanto
					<lb xml:id="P91R6" n="6" facs="#P91Riga6" />
					abusata.	
				</ab>
				<ab type="pericope" n="2" xml:id="Pag91Pericope2">
					<lb xml:id="P91R7" n="7" facs="#P91Riga7" />
					<damage agent="ink"><supplied>a</supplied></damage>llo<damage agent="ink"><supplied>ra</supplied></damage> il
					<damage agent="ink"><supplied><gap atLeast="5" atMost="9" unit="chars" reason="illegible"/></supplied></damage> che mi
					<lb xml:id="P91R8" n="8" facs="#P91Riga8" />
					resi utile.
				</ab>

				<ab type="pericope" n="3" xml:id="Pag91Pericope3">
					C'era un'automobile
					<lb xml:id="P91R9" n="9" facs="#P91Riga9" />
					verde, l'automobile dei fascisti
					<lb xml:id="P91R10" n="10" facs="#P91Riga10" />
					in piazza, con un fucile 
					<lb xml:id="P91R11" n="11" facs="#P91Riga11" />
					mitra<lb break="no"/>gliatore e molte munizioni.
				</ab>

				<ab type="pericope" n="4" xml:id="Pag91Pericope4">
					<lb xml:id="P91R12" n="12" facs="#P91Riga12" />
					Ci saltai sopra, presi con me
					<lb xml:id="P91R13" n="13" facs="#P91Riga13" />
					una recluta che era già stata
					<lb xml:id="P91R14" n="14" facs="#P91Riga14" />
					fermata dai fascisti e ora,
					<lb xml:id="P91R15" n="15" facs="#P91Riga15" />
					libera, voleva fuggire <gap reason="illegible" quantity="1" unit="chars" /> via
					<lb xml:id="P91R16" n="16" facs="#P91Riga16" />
					alle basi di <placeName>Baruge</placeName>,
				</ab>

				<ab type="pericope" n="5" xml:id="Pag91Pericope5">
					ero felice
					<lb xml:id="P91R17" n="17" facs="#P91Riga17" />
					del colpo riuscito, felice del 
					<lb xml:id="P91R18" n="18" facs="#P91Riga18" />
					pericolo superato, felice della
					<lb xml:id="P91R19" n="19" facs="#P91Riga19" />
					vittoria.
				</ab>

				<ab type="pericope" n="6" xml:id="Pag91Pericope6">
					Dopo pochi minuti la
					<lb xml:id="P91R20" n="20" facs="#P91Riga20" />
					zona di <placeName>Cavour</placeName> era lontana.
				</ab>

				<ab type="pericope" n="7" xml:id="Pag91Pericope7">
					<lb xml:id="P91R21" n="21" facs="#P91Riga21" />
					<gap atLeast="7" atMost="9" unit="chars" reason="illegible"/> la <gap atLeast="4" atMost="6" unit="chars" reason="illegible"/> fu difficile,
					<lb xml:id="P91R22" n="22" facs="#P91Riga22" />
					ma <del rend="overstrike">nella</del> la popolazione, più
				</ab>
			</div>


			<div type="pagina" n="92">
				<pb n="92" facs="#P92Intera" />
				
				<ab type="pericope" n="1" xml:id="Pag92Pericope1">

					<lb xml:id="P92R1" n="1" facs="#P92Riga1" />
					di 40 persone, corsero ad aiutare,
					<lb xml:id="P92R2" n="2" facs="#P92Riga2" />
					perché il popolo ha per i ribelli
					<lb xml:id="P92R3" n="3" facs="#P92Riga3" />
					gli stessi sentimenti che i <gap atLeast="8" atMost="10" unit="chars" reason="illegible"/> 
					<lb xml:id="P92R4" n="4" facs="#P92Riga4" />
					mostravano <damage agent="ink"><supplied>per</supplied></damage> i Garibaldini.
				</ab>

				<ab type="pericope" n="2" xml:id="Pag92Pericope2">
					
					<lb xml:id="P92R5" n="5" facs="#P92Riga5" />
					A <gap atLeast="2" atMost="3" unit="chars" reason="illegible"/> <gap atLeast="3" atMost="4" unit="chars" reason="illegible"/> 
					molte fes<damage agent="ink"><supplied>te</supplied></damage> dal
					<lb xml:id="P92R6" n="6" facs="#P92Riga6" />
					<gap atLeast="6" atMost="7" unit="chars" reason="illegible"/>, che mi abbracciò come
					<lb xml:id="P92R7" n="7" facs="#P92Riga7" />
					sempre quando si ritorna da
					<lb xml:id="P92R8" n="8" facs="#P92Riga8" />
					una bella azione, molti 
					<lb xml:id="P92R9" n="9" facs="#P92Riga9" />
					ralle<lb break="no"/>gramenti per il bottino da tutti.
				
				</ab>

				<ab type="pericope" n="3" xml:id="Pag92Pericope3">

					<lb xml:id="P92R10" n="10" facs="#P92Riga10" />
					Intanto l'azione non era
					<lb xml:id="P92R11" n="11" facs="#P92Riga11" />
					finita.
				
				</ab>

				<ab type="pericope" n="4" xml:id="Pag92Pericope4">

					Passò un'automobile.
					<lb xml:id="P92R12" n="12" facs="#P92Riga12" />
